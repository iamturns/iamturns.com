{
  "name": "iamturns.com",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Matt Turnbull <matt@iamturns.com>",
  "repository": "github:iamturns/iamturns.com.git",
  "scripts": {
    "build": "run-s styles:build gatsby:build",
    "ci": "run-s test clean build",
    "clean": "rimraf .cache/ build/ public/",
    "clean-modules": "rimraf node_modules/",
    "clean-modules:force": "rimraf node_modules/ package-lock.json",
    "develop": "run-p styles:watch gatsby:develop",
    "format": "prettier --write 'src/**/*.{js,jsx,css,scss,json,md}'",
    "gatsby:build": "gatsby build",
    "gatsby:develop": "gatsby develop --host 0.0.0.0",
    "lint": "run-p lint:*",
    "lint:js": "eslint 'src/**/*.{js,jsx}' --fix",
    "lint:styles": "stylelint 'src/**/*.css'",
    "lint:types": "flow check",
    "precommit": "flow check && lint-staged",
    "prepush": "npm run test",
    "reset": "run-s clean clean-modules install",
    "serve": "gatsby serve",
    "styles:build": "postcss src/styles/main.css -o build/styles/main.css",
    "styles:watch": "postcss src/styles/main.css -o build/styles/main.css --watch",
    "test": "run-s format lint",
    "upgrade": "run-s upgrade:semver upgrade:modules",
    "upgrade:semver": "npm run clean-modules:force && npm install",
    "upgrade:modules": "npm-check --skip-unused --update --save-exact"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "gatsby": "1.9.241",
    "gatsby-image": "1.0.42",
    "gatsby-link": "1.6.39",
    "gatsby-plugin-react-helmet": "2.0.8",
    "gatsby-plugin-sharp": "1.6.41",
    "gatsby-source-filesystem": "1.5.27",
    "gatsby-transformer-javascript-frontmatter": "1.0.8",
    "gatsby-transformer-sharp": "1.6.22",
    "lodash": "4.17.5",
    "logrocket": "0.6.10",
    "moment": "^2.21.0",
    "prop-types": "15.6.1",
    "react-helmet": "5.2.0",
    "url-join": "4.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-preset-flow": "6.23.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "flow-bin": "0.68.0",
    "husky": "0.14.3",
    "lint-staged": "7.0.0",
    "npm-check": "5.6.0",
    "npm-run-all": "4.1.2",
    "postcss-cli": "5.0.0",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.1.0",
    "postcss-nested": "3.0.0",
    "postcss-url": "7.3.1",
    "prettier": "1.11.1",
    "prettier-stylelint": "0.4.2",
    "rimraf": "2.6.2",
    "stylelint": "9.1.3",
    "stylelint-config-standard": "18.2.0",
    "tailwindcss": "0.5.1"
  }
}
